{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -b master"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.20",
    "ahooks": "^2.6.0",
    "antd": "^4.6.3",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.20",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.2.2",
    "eslint": "^7.9.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.1"
  }
}
